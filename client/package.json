{
  "name": "@gurupras/simple-signal-client",
  "version": "3.1.1",
  "description": "socket.io signalling solution for simple-peer",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "author": "Thomas Mullen",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "test": "node support/server.cjs > /dev/null | npm run test-node",
    "test-browser": "airtap -- test/*.cjs",
    "test-browser-local": "airtap --local -- test/*.cjs",
    "test-node": "WRTC=wrtc tape test/*.cjs"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cuid": "^3.0.0",
    "inherits": "^2.0.4",
    "mitt": "^3.0.1",
    "simple-peer": "^9.11.1"
  },
  "devDependencies": {
    "@types/simple-peer": "^9.11.8",
    "airtap": "^0.1.0",
    "babel-minify": "^0.5.2",
    "browserify": "^14.1.0",
    "eslint": "^9.32.0",
    "neostandard": "^0.12.2",
    "npm-run-all": "^4.1.5",
    "socket.io": "^2.2.0",
    "socket.io-client": "^1.7.3",
    "tape": "^4.6.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "wrtc": "^0.4.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  }
}